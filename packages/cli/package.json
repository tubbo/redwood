{
  "name": "@redwoodjs/cli",
  "version": "1.0.2",
  "description": "The Redwood Command Line",
  "repository": {
    "type": "git",
    "url": "https://github.com/redwoodjs/redwood.git",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "bin": {
    "redwood": "./dist/index.js",
    "rw": "./dist/index.js",
    "rwfw": "./dist/rwfw.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn build:js",
    "build:clean-dist": "rimraf 'dist/**/*/__tests__'",
    "build:js": "babel src -d dist --extensions \".js,.ts,.tsx\" --copy-files --no-copy-ignored && yarn build:clean-dist",
    "build:watch": "nodemon --watch src --ext \"js,ts,tsx,template\" --ignore dist --exec \"yarn build && yarn fix:permissions\"",
    "dev": "RWJS_CWD=../../__fixtures__/example-todo-main node dist/index.js",
    "fix:permissions": "chmod +x dist/index.js dist/rwfw.js",
    "prepublishOnly": "yarn build",
    "test": "jest src",
    "test:watch": "yarn test --watch"
  },
  "dependencies": {
    "@prisma/sdk": "3.12.0",
    "@redwoodjs/api-server": "1.0.2",
    "@redwoodjs/internal": "1.0.2",
    "@redwoodjs/prerender": "1.0.2",
    "@redwoodjs/structure": "1.0.2",
    "@redwoodjs/telemetry": "1.0.2",
    "boxen": "5.1.2",
    "camelcase": "6.3.0",
    "chalk": "4.1.2",
    "concurrently": "7.1.0",
    "configstore": "3.1.5",
    "core-js": "3.22.0",
    "cross-env": "7.0.3",
    "decamelize": "5.0.0",
    "dotenv-defaults": "5.0.0",
    "envinfo": "7.8.1",
    "execa": "5.1.1",
    "fast-glob": "3.2.11",
    "fs-extra": "10.1.0",
    "humanize-string": "2.1.0",
    "latest-version": "5.1.0",
    "listr": "0.14.3",
    "listr-verbose-renderer": "0.6.0",
    "lodash": "4.17.21",
    "param-case": "3.0.4",
    "pascalcase": "1.0.0",
    "pluralize": "8.0.0",
    "prettier": "2.8.6",
    "prisma": "3.12.0",
    "prompts": "2.4.2",
    "rimraf": "3.0.2",
    "secure-random-password": "0.2.3",
    "terminal-link": "2.1.1",
    "yargs": "16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.16.7",
    "@babel/core": "7.16.7",
    "@types/listr": "0.14.4",
    "jest": "27.5.1",
    "typescript": "4.6.3"
  },
  "gitHead": "3905ed045508b861b495f8d5630d76c7a157d8f1"
}
